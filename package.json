    "uri-template": "^1.0.0",
    "qs": "^6.2.0"
  },
  "devDependencies": {
    "jshint": "2.9.2",
    "mocha": "2.5.3",
    "should": "9.0.2",
    "istanbul": "0.4.3",
    "nock": "8.0.0"
  }
}
