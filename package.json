{
  "name": "losant-rest",
  "version": "1.5.3",
  "description": "REST client for accessing the Losant API",
  "main": "lib/index.js",
  "homepage": "https://github.com/Losant/losant-rest-js#readme",
  "author": "Losant <hello@losant.com>",
  "license": "MIT",
  "keywords": ["IoT", "platform", "api", "REST"],
  "bugs": { "url": "https://github.com/Losant/losant-rest-js/issues" },
  "directories": {
    "doc": "./docs",
    "example": "./examples",
    "lib": "./lib"
